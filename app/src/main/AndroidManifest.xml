<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ouroboros">

    <application
        android:name=".model.room.SesionRoom"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity android:name=".activities.MainActivity"></activity>
    <activity android:name=".activities.session.LoginActivity"></activity>
    <activity android:name=".activities.session.RegistryActivity"></activity>
    <activity android:name=".activities.my_topics.AddTopicActivity"></activity>
    <activity android:name=".activities.my_topics.MyTopicDetailActivity"></activity>
    <activity android:name=".activities.topics.TopicDetailActivity"></activity>
    <activity android:name=".activities.my_topics.EditTopicActivity"></activity>
    <activity android:name=".activities.account.UpdatePasswordDialog"
        android:theme="@android:style/Theme.Holo.Dialog">
    </activity>
    </application>
</manifest>